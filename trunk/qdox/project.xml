<project>

  <pomVersion>3</pomVersion>

  <name>QDox</name>
  <id>qdox</id>
  <currentVersion>1.6-SNAPSHOT</currentVersion>
  <inceptionYear>2002</inceptionYear>
  <package>com.thoughtworks.qdox</package>
  <logo>/images/qdox.gif</logo>

  <organization>
    <name>ThoughtWorks, Inc</name>
    <url>http://www.thoughtworks.com/</url>
  </organization>

  <shortDescription>QDox - Quick JavaDoc Scanner</shortDescription>
  <description>
    QDox is a high speed, small footprint parser for extracting
    class/interface/method definitions from source files complete with
    JavaDoc @tags. It is designed to be used by active code generators or
    documentation tools.
  </description>

  <url>http://qdox.codehaus.org/</url>
  <issueTrackingUrl>http://jira.codehaus.org/secure/BrowseProject.jspa?id=10103</issueTrackingUrl>

  <siteAddress>qdox.codehaus.org</siteAddress>
  <siteDirectory>/home/projects/qdox/public_html</siteDirectory>

  <distributionDirectory>/dist</distributionDirectory>
  <distributionSite>beaver.codehaus.org</distributionSite>

  <repository>
    <connection>scm:cvs:pserver:anonymous@cvs.qdox.codehaus.org:/home/projects/qdox/scm:qdox</connection>
    <developerConnection>scm:cvs:ext:${maven.username}@cvs.qdox.codehaus.org:/home/projects/qdox/scm:qdox</developerConnection>
    <url>http://cvs.qdox.codehaus.org/qdox</url>   
  </repository>
  
